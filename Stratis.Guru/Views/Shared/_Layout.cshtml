<script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-5a48282c1ee2c396"></script>
<script type="text/javascript">
	$(document).ready(function(e) {
		NProgress.done();
	});
</script>
</html>